    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
        "namespace": "default",
        "name": "room-service-service"
    },
    "spec": {
        "selector": {
            "app": "room-service"
        },
        "type": "LoadBalancer",
        "ports": [
            {
                "port": 8888,
                "protocol": "TCP",
                "targetPort": 8888
            }
        ]
    }
}